{
  "PulsarHostSettings": {
    "ServiceUrl": "pulsar://dockerhost:6650",
    "PulsarManagerBaseUrl": "http://dockerhost:7750",
    "PulsarAdminBaseUrl": "http://dockerhost:8080"
  },
  "ProducerSettings": {
      "Source": "match-started",
      "PulsarTenant": "public",
      "PulsarNamespace": "topica",
      "PulsarBlockIfQueueFull": true,
      "PulsarMaxPendingMessages": -1,
      "PulsarMaxPendingMessagesAcrossPartitions": -1,
      "PulsarEnableBatching": false,
      "PulsarEnableChunking": false,
      "PulsarBatchingMaxMessages": 10,
      "PulsarBatchingMaxPublishDelayMilliseconds": 500
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Debug",
      "Microsoft": "Debug",
      "Microsoft.Extensions.Http.DefaultHttpClientFactory": "Information"
    }
  }
}