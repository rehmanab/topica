  },
  "AzureServiceBusHostSettings": {
    "ConnectionString": "Endpoint=sb://dockerhost;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=SAS_KEY_VALUE;UseDevelopmentEmulator=true;"
  },
  "KafkaHostSettings": {
    "BootstrapServers": [
      "dockerhost:9092"
    ]
  },
  "PulsarHostSettings": {
    "ServiceUrl": "pulsar://dockerhost:6650",
    "PulsarManagerBaseUrl": "http://dockerhost:7750",
    "PulsarAdminBaseUrl": "http://dockerhost:8080"
