{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http": "Warning",
      "Microsoft.Extensions.Caching.StackExchangeRedis.RedisCache": "Information",
      "Microsoft.EntityFrameworkCore": "Warning",
      "HealthChecks.UI.Core.HostedService": "Warning"
    },
    "Console": {
      "FormatterName": "CustomTimePrefixingFormatter",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss.ffff ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "HealthCheckSettings": {
    "AwsQueue": {
      "Name": "AWS SQS Queue",
      "Tag": "Aws",
      "Enabled": true,
      "TimeOut": "00:00:15"
    },
    "AwsTopic": {
      "Name": "AWS SNS Topic",
      "Tag": "Aws",
      "Enabled": false,
      "TimeOut": "00:00:15"
    },
    "AzureServiceBus": {
      "Name": "Azure Service Bus Topic",
      "Tag": "Azure",
      "Enabled": true,
      "TimeOut": "00:00:15"
    },
    "Kafka": {
      "Name": "Kafka Topic",
      "Tag": "Kafka",
      "Enabled": false,
      "TimeOut": "00:02:00"
    },
    "Pulsar": {
      "Name": "Pulsar Topic",
      "Tag": "Pulsar",
      "Enabled": false,
      "TimeOut": "00:02:00"
    },
    "RabbitMqQueue": {
      "Name": "RabbitMq Queue",
      "Tag": "RabbitMq",
      "Enabled": true,
      "TimeOut": "00:00:15"
    },
    "RabbitMqTopic": {
      "Name": "RabbitMq Topic",
      "Tag": "RabbitMq",
      "Enabled": false,
      "TimeOut": "00:00:15"
    }
  },
  "AwsHostSettings": {
    "ProfileName": "",
    "AccessKey": "",
    "SecretKey": "",
    "ServiceUrl": "http://dockerhost:4566",
